@import 'common/style/iconfont.wxss';
@import "pages/wxParse/wxParse.wxss";
 /* @import 'common/style/weui.wxss';  */

/**app.wxss**/
.container {
  height:100%;
  display:flex;
  flex-direction:column;
  align-items:center;
  box-sizing:border-box;
  width:100%;
  font-size:28rpx;
} 

.full-width{
  width: 100%;
}

.sp-list-container{
  display:flex;
  flex-direction:column;
  width:100%;
}

.sp-list{
  min-height:88rpx;
  display:flex;
  justify-content:space-between;
  flex-direction:row;
  padding:25rpx;
  border-bottom:1px solid #e2e2e2;
}
.sp-list-title{
  display:flex;
  flex-direction:column;
  margin-right:10rpx;
}
.sp-list-name{
  flex-direction:row;
  flex-wrap:no-wrap;
  display:flex;
}
.sp-list-img{
  display:flex;
  align-items:center;
}

.sp-list-img image{
  display:flex;
}
.img-150{
  width:150rpx;
  height:150rpx;
}
.img-64{
  width: 64rpx;
  height: 64rpx;
}